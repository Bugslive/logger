{
  "name": "ts-log-debug",
  "version": "4.0.4",
  "description": "A multi channel logger written in TypeScript.",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "clean": "rimraf 'test/**/*.{js,js.map}' 'src/**/*.{js,js.map}'",
    "tslint": "tslint 'src/**/*.ts?(x)'",
    "test": "npm run tslint && NODE_ENV=test nyc mocha",
    "test:unit": "mocha --recursive",
    "coveralls:travis": "nyc report --reporter=text-lcov | coveralls",
    "tsc": "tsc --project tsconfig.json",
    "tsc:compile": "rimraf ./lib && tsc --project tsconfig.compile.json",
    "doc:serve": "concurrently \"tsc --project tsconfig.compile.json -w\" \"docsify serve ./docs\" \"cd docs/_build && nodemon --delay 2.5 -e ts --watch ../../lib build-api.js\"",
    "doc:build": "cd docs/_build && node build-api.js && cd ../..",
    "prepublish": "npm run tsc:compile"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/romakita/ts-log-debug.git"
  },
  "keywords": [
    "ts",
    "log",
    "debug",
    "typescript",
    "trace"
  ],
  "author": "Romakita",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/romakita/log-debug/issues"
  },
  "homepage": "https://github.com/romakita/log-debug",
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/chai-as-promised": "7.1.0",
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.11.0",
    "@types/sinon": "^7.0.9",
    "@types/sinon-chai": "^3.2.2",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.0.0",
    "concurrently": "^4.1.0",
    "coveralls": "^3.0.3",
    "docsify-cli": "^4.2.1",
    "docsify-server-renderer": "^4.7.0",
    "mocha": "^6.0.2",
    "nyc": "^13.3.0",
    "rimraf": "^2.6.3",
    "semantic-release": "^8.2.0",
    "sinon": "^7.2.7",
    "sinon-chai": "^3.3.0",
    "source-map-support": "^0.5.10",
    "ts-node": "^8.0.3",
    "tslint": "^5.13.1",
    "typescript": "^3.3.3333"
  },
  "peerDependencies": {
    "source-map-support": "^0.5.10"
  },
  "dependencies": {
    "colors": "^1.3.3",
    "date-format": "^2.0.0",
    "semver": "^5.6.0",
    "streamroller": "^1.0.3",
    "tslib": "^1.9.3"
  },
  "directories": {
    "lib": "lib",
    "src": "src",
    "test": "test"
  },
  "release": {
    "debug": false,
    "branch": "production"
  },
  "gflow": {
    "master": "master",
    "production": "production"
  }
}
